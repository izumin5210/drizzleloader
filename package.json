  },
  "packageManager": "pnpm@10.28.2",
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/node": "^25.1.0",
    "dataloader": "^2.2.3",
    "drizzle-orm": "^0.45.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
